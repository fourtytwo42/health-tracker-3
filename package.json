{
  "name": "ai-health-companion",
  "version": "1.0.0",
  "description": "AI-driven health management platform with chat-first interface",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:api": "vitest run --config vitest.api.config.ts",
    "test:db": "vitest run --config vitest.db.config.ts",
    "test:ui": "vitest run --config vitest.ui.config.ts",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "test:performance": "node scripts/performance-test.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "node scripts/seed.ts",
    "db:seed-usda": "node scripts/seed-usda-ingredients.js",
    "db:seed-all": "node scripts/seed-all-data.js",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:reset-and-seed": "prisma migrate reset --force && node scripts/seed-all-data.js",
    "db:fix-calories": "node scripts/fix-calories.js",
    "lighthouse": "lighthouse-ci autorun",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup": "npm run docker:up && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.470.0",
    "@axe-core/react": "^4.10.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@modelcontextprotocol/sdk": "^1.12.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@mui/material-nextjs": "^7.2.0",
    "@mui/x-charts": "^6.18.0",
    "@prisma/client": "^5.7.1",
    "@tabler/icons-react": "^2.46.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "express-rate-limit": "^7.1.5",
    "framer-motion": "^10.16.16",
    "helmet": "^7.1.0",
    "idb-keyval": "^6.2.1",
    "jsonwebtoken": "^9.0.2",
    "mcp-handler": "^1.0.0",
    "next": "14.0.4",
    "next-pwa": "^5.6.0",
    "next-translate": "^2.6.2",
    "pg": "^8.16.3",
    "prisma": "^5.7.1",
    "prom-client": "^15.1.0",
    "puppeteer": "^22.0.0",
    "quick-lru": "^7.0.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "recharts": "^2.8.0",
    "swr": "^2.2.4",
    "workbox-window": "^7.0.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.0.4",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "playwright": "^1.40.1",
    "prettier": "^3.1.1",
    "react-test-renderer": "^18.2.0",
    "supertest": "^6.3.3",
    "testcontainers": "^10.7.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
